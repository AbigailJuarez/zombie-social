<% include _header%>
 
<h1>Registro de Armas</h1>

<form action="/equipamento" method="post">
    <div class="form-group">
        <input name="descripcion" type="text" class="form-control"
        placeholder="Descripcion" required autofocus/>
    </div>
    <div class="form-group">
        <input name="defensa" type="text" class="form-control"
        placeholder="Defensa" required autofocus/>
    </div>
    <div class="form-group">
        <input name="categoria" type="text" class="form-control"
        placeholder="Categoria" required autofocus/>
    </div>
    <div class="form-group">
        <input name="peso" type="text" class="form-control"
        placeholder="Peso" required autofocus/>
    </div>

    <input type="submit" value="Registrar" class="btn btn-primary btn-block">
</form>

<%include _footer%>
